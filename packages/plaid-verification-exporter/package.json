    "date-fns-tz": "^3.2.0",
    "dotenv": "^16.4.1",
    "plaid": "^18.2.0"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/node": "^20.11.19",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "plaid",
    "identity",
    "verification",
    "export"
  ],
