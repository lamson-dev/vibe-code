{
  "name": "@vibe-code/compliance-report-tool",
  "version": "1.0.0",
  "description": "Tool for generating compliance reports from various data sources",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "NODE_OPTIONS='--loader ts-node/esm' ts-node src/index.ts",
    "export:all": "NODE_OPTIONS='--loader ts-node/esm' ts-node src/index.ts",
    "plaid:export": "tsx src/plaid-exporter.ts",
    "plaid:process": "tsx src/plaid-process-csv.ts",
    "plaid:test": "tsx src/test-plaid.ts",
    "middesk:export": "tsx src/middesk-exporter.ts",
    "middesk:process": "tsx src/middesk-process-csv.ts",
    "middesk:test": "tsx src/test-middesk.ts",
    "test": "jest"
  },
  "dependencies": {
    "axios": "^1.8.4",
    "csv-parse": "^5.5.3",
    "csv-stringify": "^6.4.5",
    "date-fns": "^3.3.1",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^16.4.1",
    "plaid": "^18.2.0"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/node": "^20.11.19",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "plaid",
    "identity",
    "verification",
    "export"
  ],
  "author": "",
  "license": "ISC"
}
